echo Starting Instapapure server

CP=.
CP=$CP:lib/commons-io-1.4.jar
CP=$CP:lib/commons-codec-1.3.jar
CP=$CP:lib/commons-fileupload-1.2.1.jar
CP=$CP:lib/servlet-api-2.5-20081211.jar
CP=$CP:lib/hsqldb.jar
CP=$CP:lib/jetty-util-6.1.15.jar
CP=$CP:lib/jetty-6.1.15.jar
CP=$CP:lib/grizzly-http-webserver-1.9.10.jar
CP=$CP:lib/jline-0.9.94.jar
CP=$CP:lib/clojure-contrib.jar
CP=$CP:lib/compojure.jar
CP=$CP:lib/clojure.jar

#echo $CP
java -cp $CP jline.ConsoleRunner clojure.lang.Repl instapapure.clj